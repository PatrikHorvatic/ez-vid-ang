{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "ez-vid-ang": {
      "projectType": "library",
      "root": "projects/ez-vid-ang",
      "sourceRoot": "projects/ez-vid-ang/src",
      "prefix": "eva",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ez-vid-ang/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ez-vid-ang/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/ez-vid-ang/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "style": "scss",
      "standalone": false,
      "type": "component"
    },
    "@schematics/angular:directive": {
      "standalone": false,
      "type": "directive"
    },
    "@schematics/angular:service": {}
  }
}